<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Link to the manifest format reference: https://gerrit.googlesource.com/git-repo/+/HEAD/docs/manifest-format.md -->
  <default remote="yocto" revision="master"/>

  <remote name="yocto" fetch="git://git.yoctoproject.org" />
  <remote name="oe" fetch="git://git.openembedded.org" />
  <remote name="kraj" fetch="git://github.com/kraj" />
  <remote name="igalia" fetch="git://github.com/Igalia" />

  <project name="poky" revision="zeus" remote="yocto" />
  <project name="meta-openembedded" revision="zeus"  remote="oe" path="meta-openembedded">
    <linkfile src="." dest="poky/meta-openembedded" />
  </project>
  <project name="meta-clang" revision="zeus"  remote="kraj" path="meta-clang">
    <linkfile src="." dest="poky/meta-clang" />
  </project>
  <project name="meta-raspberrypi" revision="zeus"  remote="yocto" path="meta-raspberrypi">
    <linkfile src="." dest="poky/meta-raspberrypi" />
  </project>
  <project name="meta-security" revision="zeus"  remote="yocto" path="meta-security">
    <linkfile src="." dest="poky/meta-security" />
  </project>
  <project name="meta-webkit" revision="zeus"  remote="igalia" path="meta-webkit">
    <linkfile src="." dest="poky/meta-webkit" />
  </project>
</manifest>
